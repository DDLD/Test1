<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="User.UserEntity" table="user" schema="test">
        <id name="userPhone" column="User_Phone"/>
        <property name="userPassword" column="User_password"/>
        <property name="userSex" column="User_sex"/>
        <property name="userDate" column="User_date"/>
        <property name="userNational" column="User_national"/>
        <property name="userInfo" column="User_info"/>
    </class>
</hibernate-mapping>